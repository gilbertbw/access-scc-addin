Option Compare Database
Option Explicit

Private Sub btnImport_Click()
  If Me.chkImport.Value = True Then
    SCC_Update Me.txtOutputLocation.Value, Me.chkShowUpdates.Value
  End If
  
  ' put all the details in the tmpdatabase
  asPurgeTmpObjectsTables
  asPopulateTmpObjectsTables (asImport)
  
  ' Select which objects to import
  DoCmd.OpenForm "asSelectObjects", acNormal, , , , acWindowNormal, "import"
  
  UpdateVersionDetails
  Application.RunCommand acCmdSaveRecord
End Sub

Private Sub btnOpenFolder_Click()
  Shell "Explorer.exe " & CStr(Me.txtOutputLocation.Value), vbNormalFocus
End Sub

Private Sub cboSCCSystem_AfterUpdate()
  SetSCCSystem Me.cboSCCSystem
  Select Case Me.cboSCCSystem
    Case sccSystemSubversion
      chkCommit.Enabled = True
      chkCommit_AfterUpdate
      chkImport.Enabled = True
      chkImport_AfterUpdate
      
      Me.Label38.Visible = True
      Me.Box37.Visible = True
      Me.txtLastImported.Visible = True
      Me.txtLastVersion.Visible = True
      Me.txtFolderVersion.Visible = True
      Me.txtLatestVersion.Visible = True
      Me.chkRepository.Visible = True
      
    Case sccSystemText
      chkCommit = True
      chkCommit.Enabled = False
      chkShowCommit = False
      chkShowCommit.Enabled = False
      chkNoImportExternalChanges = True
      chkNoImportExternalChanges.Enabled = False
      Application.RunCommand acCmdSaveRecord
      
      chkImport = False
      chkImport.Enabled = False
      chkImport_AfterUpdate
      
      Me.Label38.Visible = False
      Me.Box37.Visible = False
      Me.txtLastImported.Visible = False
      Me.txtLastVersion.Visible = False
      Me.txtFolderVersion.Visible = False
      Me.txtLatestVersion.Visible = False
      Me.chkRepository.Visible = False
  End Select
  
  UpdateVersionDetails
End Sub

Private Sub chkCommit_AfterUpdate()
  Me.chkShowCommit.Enabled = Me.chkCommit.Value
  If Me.chkShowCommit.Value = False Then Me.chkShowCommit.Value = False
  Me.chkNoImportExternalChanges.Enabled = Me.chkCommit.Value
  Me.chkNoImportExternalChanges.Value = False
  Application.RunCommand acCmdSaveRecord
End Sub

Private Sub chkImport_AfterUpdate()
  Me.chkShowUpdates.Enabled = Me.chkImport.Value
  If Me.chkImport.Value = False Then Me.chkShowUpdates.Value = False
  Application.RunCommand acCmdSaveRecord
End Sub

Private Sub chkNoImportExternalChanges_Click()
  If Me.chkNoImportExternalChanges = True Then
    MsgBox "Checking this option will stop any changes made to objects/files after they have been exported from Access, from being imported back into Access again." & vbCrLf & vbCrLf & _
           "An example usage of this option, is when you wish to perform a partial commit of a module." & vbCrLf & vbCrLf & _
           "You should take care that your database is kept in sync with the repository.", vbExclamation
  End If
End Sub

Private Sub chkRepository_AfterUpdate()
  Application.RunCommand acCmdSaveRecord
End Sub

Private Sub chkSelectAllObjects_Click()
  If Me.chkSelectAllObjects.Value = True Then
    Me.chkQueries.Value = True
    Me.chkForms.Value = True
    Me.chkReports.Value = True
    Me.chkMacros.Value = True
    Me.chkModules.Value = True
    Me.chkTables.Value = True
    Me.chkCmdBars.Value = True
    Me.chkExtras.Value = True
    Me.chkReferences.Value = True
    Me.chkImportExport.Value = True
  Else
    Me.chkQueries.Value = False
    Me.chkForms.Value = False
    Me.chkReports.Value = False
    Me.chkMacros.Value = False
    Me.chkModules.Value = False
    Me.chkTables.Value = False
    Me.chkCmdBars.Value = False
    Me.chkExtras.Value = False
    Me.chkReferences.Value = False
    Me.chkImportExport.Value = False
  End If
End Sub

Private Sub cmdCompactDatabase_Click()
  CommandBars("Menu Bar").Controls("Tools").Controls("Database utilities").Controls("Compact and repair database...").accDoDefaultAction
End Sub

Private Sub cmdFolderDialog_Click()
  Dim strFolderPath As String

  strFolderPath = BrowseFolderDialog(Me.hwnd, "Select SVN Checkout Folder", 0, Me.txtOutputLocation.Value)
  If strFolderPath <> "" Then
    Me.txtOutputLocation = strFolderPath
  End If
  Call txtOutputLocation_AfterUpdate
End Sub

Private Sub cmdRemoveGUIDs_Click()
  
  DoCmd.Hourglass True
  If CleanFormsReports(Me.txtOutputLocation.Value) Then
    SetConfigValue "Remove GUIDs", True, Me.txtOutputLocation.Value
    DoCommit ' bring up SCC Commit window
    Me.cmdRun.SetFocus
    Me.cmdRemoveGUIDs.Enabled = False
  Else
    MsgBox "Removing GUIDs from Forms and Reports has failed."
  End If
  DoCmd.Hourglass False
End Sub

Private Sub cmdRepairPrinter_Click()
  Call FindFormPrinterExports(Me.txtOutputLocation)
  Call FindReportPrinterExports(Me.txtOutputLocation)
End Sub

Private Sub cmdRun_Click()
  ' put all the details in the tmpdatabase
  asPurgeTmpObjectsTables
  asPopulateTmpObjectsTables (asExport)

  ' Select which objects to export
  DoCmd.OpenForm "asSelectObjects", acNormal, , , , acWindowNormal, "export"
  UpdateVersionDetails
  Application.RunCommand acCmdSaveRecord
End Sub

Private Sub cmdUTF8Encoding_Click()
  Dim i As Long
  Dim j As Long
  Dim strFoldername As String
  Dim strFilename As String
  Dim arrSVNItems() As SVNITEM
  Dim strType As String
  
  DoCmd.Hourglass True
  For i = 0 To 3
    Select Case i
      Case 0: strType = "Forms"
      Case 1: strType = "Reports"
      Case 2: strType = "Macros"
      Case 3: strType = "Queries"
    End Select
    strFoldername = appendSlash(Me.txtOutputLocation.Value) & strType
      
    SCC_GetAllVersionsSubFolder strFoldername, arrSVNItems
    
    If (Not arrSVNItems) <> -1 Then ' check that the arr is not nothing before looping through it
      For j = LBound(arrSVNItems) To UBound(arrSVNItems)
        If arrSVNItems(j).svName <> "" Then
          strFilename = appendSlash(strFoldername) & arrSVNItems(j).svName
          If Not ConvFile_to_UTF8(strFilename) Then
            MsgBox "Conversion of '" & strFilename & "' to UTF-8 has failed."
            DoCmd.Hourglass False
            Exit Sub
          End If
        End If
      Next
    End If
  Next
  
  SetConfigValue "UTF-8 Encoding", True, Me.txtOutputLocation.Value
  
  DoCommit ' bring up SCC Commit window
    
  Me.cmdRun.SetFocus
  Me.cmdUTF8Encoding.Enabled = False
  DoCmd.Hourglass False
End Sub

Private Sub cmdUTF8FullEncode_Click()
  Dim i As Long
  Dim j As Long
  Dim strFoldername As String
  Dim strFilename As String
  Dim arrSCCItems() As SVNITEM
  Dim strType As String
  
  DoCmd.Hourglass True
  For i = 0 To 6
    Select Case i
      Case 0: strType = "Forms"
      Case 1: strType = "Reports"
      Case 2: strType = "Macros"
      Case 3: strType = "Queries"
      Case 4: strType = "Tables"
      Case 5: strType = "Toolbars"
      Case 6: strType = "Extras"
    End Select
    strFoldername = Me.txtOutputLocation.Value & "/" & strType
      
    SCC_GetAllVersionsSubFolder strFoldername, arrSCCItems
    
    If (Not arrSCCItems) <> -1 Then ' check that the arr is not nothing before looping through it
      For j = LBound(arrSCCItems) To UBound(arrSCCItems)
        If arrSCCItems(j).svName <> "" Then
          strFilename = appendSlash(strFoldername) & arrSCCItems(j).svName
          If Not ConvFile_to_UTF8(strFilename) Then
            MsgBox "Conversion of '" & strFilename & "' to UTF-8 has failed."
            DoCmd.Hourglass False
            Exit Sub
          End If
        End If
      Next
    End If
  Next
  
  SetConfigValue "UTF-8 Encode All", True, Me.txtOutputLocation.Value
    
  Me.cmdRun.SetFocus
  Me.cmdUTF8FullEncode.Enabled = False
  Me.cmdUTF8Encoding.Enabled = False
  
  DoCmd.Hourglass False
End Sub

Private Sub Form_GotFocus()
  UpdateVersionDetails
  pvt_CheckVersionWarnings
  Me.txtDBFile = getCurrentDBSize()
  Application.RunCommand acCmdSaveRecord
End Sub

Private Sub Form_Load()
  Dim lngFileSize As Long
  Dim strUnits As String
  Dim strRecordSource As String

  DoCmd.OpenForm "frmSplash", acNormal, , , , acWindowNormal
  DoCmd.Hourglass True

  ' We must SetFocus to any control that can
  Me.txtOutputLocation.SetFocus

  DoCmd.Hourglass True
  strRecordSource = "SELECT ExportPath, CommitAfterExport, UpdateBeforeImport, QueriesFlag, FormsFlag, ReportsFlag, MacrosFlag, ModulesFlag, TablesFlag, ToolbarsFlag, ExtrasFlag, ReferencesFlag, ImExSpecsFlag, RelationshipsFlag, LastImportDate, LastCheckoutVersion, CheckRepoOnSartup, ShowCommitWindow, ShowUpdateWindow, SccSystem FROM MSysSCCPrefs IN '" & CurrentDb().Name & "'"

  Me.RecordSource = strRecordSource
  
  ' set default to use SVN
  If IsNull(Me.cboSCCSystem) Then
    Me.cboSCCSystem = sccSystemSubversion
  End If
  
  SetSCCSystem Me.cboSCCSystem
  
  If Me.cboSCCSystem <> sccSystemSubversion Then
    ' if not SVN the control states may need to be updated
    cboSCCSystem_AfterUpdate
  End If

  Call SCC_IsSupportedClient  ' Throw any warnings before doing anything

  cboSCCSystem_AfterUpdate
  If Me.chkRepository = True And Me.txtLastVersion.Value <> "" And Me.txtLastVersion.Value <> -1 Then
    ' This is not first use of this DB, and startup check is enabled
    pvt_CheckVersionWarnings
  Else
    Me.txtLatestVersion.Value = ""
    Me.txtSvnUrl = ""
    Me.txtFolderVersion = ""
  End If

  Me.txtDBFile = getCurrentDBSize()
  Forms![frmSplash].TimerInterval = 3000  ' This will close the splash screen in 3 seconds
End Sub

Private Function getCurrentDBSize() As String
  Dim lngFileSize As Long
  Dim strUnits As String
  
  lngFileSize = FileLen(CurrentDb.Name)
  lngFileSize = lngFileSize / 1024 ' KB
  strUnits = "Kb"
  If lngFileSize > 9999 Then
    lngFileSize = lngFileSize / 1024  ' MB
    strUnits = "Mb"
  End If
  If lngFileSize > 9999 Then
    lngFileSize = lngFileSize / 1024  ' GB
    strUnits = "Gb"
  End If
  getCurrentDBSize = str$(lngFileSize) & " " & strUnits
End Function

Private Sub txtOutputLocation_AfterUpdate()
  UpdateVersionDetails
  pvt_CheckVersionWarnings
  Application.RunCommand acCmdSaveRecord
End Sub

Public Sub UpdateVersionDetails()
  Dim str As String
  
  Me.txtLastImported = asGetLatestImportDate()
  Me.txtSvnUrl = SCC_GetMappedUrl(Me.txtOutputLocation.Value)
  If LenB(Nz(Me.txtSvnUrl.Value, vbNullString)) = 0 Or Left$(Me.txtSvnUrl.Value, 10) = "No mapped " Then ' No mapped url, cannot import, but allow export for original export
    Me.btnImport.Enabled = False ' should probably trap for when the button has focus but cannot see how user could get to that point
  Else
    Me.btnImport.Enabled = True
  End If
  Me.cmdUTF8Encoding.Enabled = Not (GetConfigValue("UTF-8 Encoding", Me.txtOutputLocation.Value) Or GetConfigValue("UTF-8 Encode All", Me.txtOutputLocation.Value))
  Me.cmdUTF8FullEncode.Enabled = Not GetConfigValue("UTF-8 Encode All", Me.txtOutputLocation.Value)
  Me.cmdRemoveGUIDs.Enabled = Not GetConfigValue("Remove GUIDs", Me.txtOutputLocation.Value)

  'txtLastVersion cannot be empty string, so protect against it
  str = SCC_GetLatestImportVersion()
  If str <> "" Then
    Me.txtLastVersion = str
  Else
    Me.txtLastVersion = "-1"
  End If
  
  Me.txtLatestVersion = SCC_GetLatestVersion(Me.txtOutputLocation.Value)
  Me.txtFolderVersion = SCC_GetWorkingFolderVersion(Me.txtOutputLocation.Value)
End Sub

Private Sub pvt_CheckVersionWarnings()
  If Me.txtLatestVersion.Value <> "" And Me.txtLastVersion.Value <> "" And IsNumeric(Me.txtLatestVersion.Value) And IsNumeric(Me.txtLastVersion.Value) Then
    ' This is not first use, and there is some version details populated
    If CLng(Me.txtLatestVersion.Value) > CLng(Me.txtFolderVersion.Value) Then
      MsgBox "The SCC Repository contains changes which are not reflected in the working copy folder." & vbCrLf & vbCrLf & _
             "When you commit to the repository, the repository version is updated," & vbCrLf & _
             "however, your working copy remains at the old version, until you perform an update.", vbExclamation
    ElseIf CLng(Me.txtFolderVersion.Value) > CLng(Me.txtLastVersion.Value) And Me.txtLastVersion.Value <> -1 Then
      MsgBox "The working copy contains changes which are not reflected in the Access build." & vbCrLf & vbCrLf & _
             "You should perform an update/import to pull these changes into Access.", vbExclamation
      Me.txtLatestVersion.ForeColor = 255
    End If
    
    ' Now set the colours
    If CLng(Me.txtLatestVersion.Value) > CLng(Me.txtFolderVersion.Value) Then
      Me.txtFolderVersion.ForeColor = 255
    Else
      Me.txtFolderVersion.ForeColor = 0
    End If
    
    If CLng(Me.txtFolderVersion.Value) > CLng(Me.txtLastVersion.Value) And Me.txtLastVersion.Value <> -1 Then
      Me.txtLastVersion.ForeColor = 255
    Else
      Me.txtLastVersion.ForeColor = 0
    End If
    Me.txtLatestVersion.ForeColor = 0 ' Repository
  End If
End Sub